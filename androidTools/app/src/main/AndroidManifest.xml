<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jay.test2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.TargetActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.EditActivity" />
        <activity android:name=".activity.ExpandActivity" />
        <activity android:name=".activity.FastAnimationActivity" />
        <activity android:name=".activity.NumRunActivity" />
        <activity android:name=".activity.ScannerActivity" />
        <activity android:name=".activity.ShowQRCodeContentActivity" />

        <meta-data
            android:name="android.max_aspect"
            android:value="2.1" />

        <activity android:name=".activity.NoteActivity" />
        <activity android:name=".activity.AidlActivity" />
        <activity android:name=".activity.CheckViewActivity"></activity>
    </application>

</manifest>